{
  "extends": [
    "@remix-run/eslint-config", 
    "@remix-run/eslint-config/node", 
    "wesbos/typescript"
  ],
  "rules": {
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "semi": false
      }
    ]
  }
}
